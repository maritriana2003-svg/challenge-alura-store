        "import pandas as pd\n",
        "import requests\n",
        "\n",
        "# 1. EXTRACCIÓN (Extract)\n",
        "url = \"URL_DE_LA_API_TELECOM_X\" # Reemplaza con la URL real del desafío\n",
        "response = requests.get(url)\n",
        "\n",
        "if response.status_code == 200:\n",
        "    data = response.json()\n",
        "    df = pd.DataFrame(data)\n",
        "    print(\"Datos extraídos exitosamente\")\n",
        "\n",
        "# 2. TRANSFORMACIÓN (Transform) - Ejemplo de limpieza\n",
        "df.drop_duplicates(inplace=True)\n",
        "# Convertir fechas (ajusta el nombre de la columna según la API)\n",
        "# df['fecha'] = pd.to_datetime(df['fecha'])\n",
        "\n",
        "# 3. CARGA (Load) - Guardar resultado para GitHub\n",
        "df.to_csv('telecom_clean.csv', index=False)"
